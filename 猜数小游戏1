int num =(int)((Math.random()*1000+1)/20);
System.out.println(num);

Scanner scan = new Scanner(System.in);
System.out.println("猜吧！");
int guess = scan.nextInt();

while (guess != num){
    if(guess>num){
        System.out.println("猜大了！");
    }else{
        System.out.println("猜小了！");
    }
    System.out.println("猜吧!");
    guess = scan.nextInt();
}
